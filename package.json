{
  "name": "flaskshop",
  "version": "2.0.0",
  "description": "a shop project with flask",
  "scripts": {
    "build": "set NODE_ENV=production && webpack --color",
    "start": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-dev-server\" \"npm run flask-server\"",
    "webpack-dev-server": "NODE_ENV=debug webpack-dev-server --port 2992 --hot --inline --mode development",
    "flask-server": "flask run",
    "lint": "eslint \"assets/js/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hjlarry/flaskshop.git"
  },
  "author": "hjlarry",
  "license": "BSD-3-Clause",
  "engines": {
    "node": ">=4"
  },
  "bugs": {
    "url": "https://github.com/hjlarry/flaskshop/issues"
  },
  "homepage": "https://github.com/hjlarry/flaskshop#readme",
  "dependencies": {
    "bootstrap": "^5.2.0",
    "daterangepicker": "^3.1.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.1",
    "jquery.cookie": "^1.4.1",
    "lazysizes": "^5.3.2",
    "select2": "^4.1.0-rc.0",
    "svg-injector-2": "^2.1.5",
    "sweetalert": "^2.1.2",
    "wangeditor": "^4.7.15"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "glob": "^8.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-loader": "^7.0.1",
    "sass": "^1.54.8",
    "sass-loader": "^13.0.2",
    "webpack": "^5.74.0",
    "webpack-bundle-tracker": "^1.6.0",
    "webpack-cli": "^4.10.0"
  }
}